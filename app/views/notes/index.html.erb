<p id="notice"><%= notice %></p>

<h1>Listing Notes</h1>

<table>
  <thead>
    <tr>
      <th>Teacher id</th>
      <th>Student id</th>
      <th>Note date</th>
      <th>Note subject</th>
      <th>Note type</th>
      <th>Penalty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notes.each do |note| %>
      <tr>
        <td><%= note.teacher_ID %></td>
        <td><%= note.student_ID %></td>
        <td><%= note.Note_Date %></td>
        <td><%= note.Note_Subject %></td>
        <td><%= note.Note_Type %></td>
        <td><%= note.Penalty %></td>
        <td><%= link_to 'Show', note %></td>
        <td><%= link_to 'Edit', edit_note_path(note) %></td>
        <td><%= link_to 'Destroy', note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
      
    <% end %>
   
  </tbody>

</table>
 <%= will_paginate @notes, renderer: BootstrapPagination::Rails, previous_label: "Older", next_label: "Newer", inner_window: 1, outer_window: 0 %>


<br>

<%= link_to 'New Note', new_note_path %>
